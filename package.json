  "bugs": {
    "url": "https://github.com/bmd-studio/lassie-wordpress-plugin/issues"
  },
  "homepage": "https://github.com/bmd-studio/lassie-wordpress-plugin#readme",
  "devDependencies": {
    "cross-env": "^7.0.2"
  }
}
